target_host=x86_64-w64-mingw32

[settings]
os=Windows
os_build=Linux
arch=x86_64
arch_build=x86_64
compiler=gcc
compiler.version=9.3
compiler.libcxx=libstdc++11
build_type=Release


[options]

[build_requires]

[env]
CHOST=$target_host
AR=$target_host-ar
AS=$target_host-as
RANLIB=$target_host-ranlib
STRIP=$target_host-strip
RC=$target_host-windres
LD=$target_host-ld
CC=$target_host-gcc
CXX=$target_host-g++