target_host=arm-linux-gnueabihf

[settings]
os=Linux
os_build=Linux
arch=armv7hf
arch_build=x86_64
compiler=gcc
compiler.version=10
compiler.libcxx=libstdc++11
build_type=Release

[options]

[build_requires]

[env]
CHOST=$target_host
# https://stackoverflow.com/questions/55204593/how-to-specify-cmake-version-used-by-vcpkg
# https://github.com/microsoft/vcpkg/issues/9570
USE_DEV_URANDOM=1
AR=$target_host-ar
AS=$target_host-as
RANLIB=$target_host-ranlib
STRIP=$target_host-strip
LD=$target_host-ld
CC=$target_host-gcc-10
CXX=$target_host-g++-10