toolchain_root=/opt/toolchains/nano-duo2/4.9.3
toolchain_prefix=arm-linux
target_host=arm-linux-gnueabihf
toolchain_version=4.9

[settings]
os=Linux
os_build=Linux
arch=armv7hf
arch_build=x86_64
compiler=gcc
compiler.version=$toolchain_version
compiler.libcxx=libstdc++11
build_type=Release

[options]

[build_requires]

[env]
CONAN_CMAKE_FIND_ROOT_PATH=/opt/toolchains/nano-duo2/rootfs
PATH=[ $toolchain_root/bin ]
CHOST=$target_host
USE_DEV_URANDOM=1
AR=$toolchain_prefix-ar
AS=$toolchain_prefix-as
RANLIB=$toolchain_prefix-ranlib
STRIP=$toolchain_prefix-strip
LD=$toolchain_prefix-ld
CC=$toolchain_prefix-gcc
CXX=$toolchain_prefix-g++
